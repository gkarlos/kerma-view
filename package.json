{
  "name": "kerma-view",
  "description": "A desktop client for Kerma",
  "version": "1.0.0",
  "author": "George Karlos",
  "license": "Apache-2.0",
  "homepage": "https://gkarlos.github.io/kerma",
  "bugs": "https://github.com/gkarlos/kerma-view/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/gkarlos/kerma-view"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "main": "./src/main/main.js",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "docs:build:linux": "KERMA_VIEW_ROOT=$(pwd) ./scripts/build-the-docs.sh",
    "docs:build:win": "powershell -ExecutionPolicy Bypass ./node_modules/.bin/jsdoc -c ./docs/conf.json -d ./docs/dev -R ./docs/README.md",
    "docs:read:linux": "KERMA_VIEW_ROOT=$(pwd) ./scripts/read-the-docs.sh",
    "docs:read:win": "powershell Invoke-Item docs/dev/index.html",
    "clean": "./scripts/clean.sh",
    "deps": "node scripts/build-external-deps.js"
  },
  "dependencies": {
    "@alexispuga/jsdoc-template": "^1.1.0",
    "@mocha/docdash": "^2.1.2",
    "@playcanvas/jsdoc-template": "^1.0.12",
    "ace-builds": "^1.4.11",
    "bootstrap": "^4.4.1",
    "clean-jsdoc-theme": "^2.2.11",
    "cli-color": "^2.0.0",
    "commander": "^5.0.0",
    "cpb-jsdoc-template": "^1.2.2",
    "daybrush-jsdoc-template": "^1.6.0",
    "docdash-elegant": "^1.1.3",
    "jquery": "^3.5.0",
    "monaco-editor": "^0.20.0",
    "monaco-loader": "^1.0.0",
    "ub-jsdoc": "^2.3.1"
  },
  "devDependencies": {
    "@migenius/jsdoc-template": "^1.0.1",
    "agility-jsdoc-template": "^1.0.3",
    "docma": "^3.2.2",
    "electron": "^8.2.1",
    "fucazu-minami": "^1.1.7",
    "jsdoc": "^3.6.4",
    "jsdoc-baseline": "^0.1.5",
    "minami": "^1.2.3",
    "namis": "^1.0.3",
    "smx-jsdoc-template": "^1.2.3",
    "sutro-jsdoc-theme": "^1.1.0",
    "tidy-jsdoc": "^1.4.0"
  }
}
