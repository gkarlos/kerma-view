{
  "plugins"   : ["plugins/markdown", "node_modules/better-docs/category"],
  "templates" : {
    "tabNames": {
      "api": "API",
      "tutorials": "Examples"
    },
    "name": "KermaView",
    "monospaceLinks" : true,
    "cleverlinks": true,
    "showInheritedInNav": true,
    "useLongnameInNav" : true,
    "useCollapsibles": true,
    "logo": {
      "url": "https://miro.medium.com/max/1400/1*OtfM8fdvOaIT3OlaUfZBjw.jpeg",
      "width": "150px",
      "height": "13px",
      "link": "https://github.com/nhnent/tui.jsdoc-template"
    },
    "footerText": "Part of the Kerma-Project"
  },
  "source" : {
    "include" : ["./src/"]
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/dev",
    "title": "KermaView",
    "template": "node_modules/better-docs"
  },
  "tags": {
    "allowUnknownTags": ["category"]
  }
}

